NEW
  PREFIX
AUTO 4,1
#!/BIN/SH
#
#
#
echo \f IFDIRTEST Script
echo \n\n
echo Testing for this Dir
IF [ -D /PKA2OSX/TESTS ]
 Echo Found Tests by full path
ELSE
 Echo Tests Not Found
FI
echo Testing for /BLANK
IF [ -D /BLANK ]
 Echo Found /Blank
ELSE
 Echo No /BLANK
FI
MAN
TEXT /MAKE/USR/SHARE/TESTS/IFDIRTEST
