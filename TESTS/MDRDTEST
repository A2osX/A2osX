#!/bin/shell
ECHO MD AND RD TEST
IF [ -D TESTDIR ] 
  ECHO The TESTDIR already exists
ELSE
  ECHO The TESTDIR is missing, will create
  MD TESTDIR
FI
ECHO Now we will remove TESTDIR
IF [ -D TESTDIR ] 
  RD TESTDIR 
ELSE
  ECHO The TESTDIR is missing, THIS SHOULD NOT BE
FI
  
ECHO RD MD TEST Complete
